{
  "name": "binary-layout",
  "version": "1.2.0",
  "license": "Apache-2.0",
  "author": "nonergodic",
  "description": "Typescript-native, declarative DSL for working with binary data",
  "keywords": [
    "typescript",
    "binary",
    "serialization",
    "deserialization",
    "parser",
    "encoder",
    "decoder",
    "Uint8Array",
    "buffer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nonergodic/layout.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "jest && tsup",
    "build": "tsup",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
